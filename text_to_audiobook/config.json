{
  "chapter_pattern": "^Chapter [IVX]+$",
  "title_pattern": null,
  "title_line_offset": 1,
  "skip_empty_lines": false,
  "max_title_search_lines": 3,
  "header_lines_count": 3,
  "ignore_case": true,
  "sub_chapter": {
    "max_reading_minutes": 5,
    "words_per_minute": 200,
    "enable_splitting": true,
    "min_paragraphs_per_sub": 2
  }
}